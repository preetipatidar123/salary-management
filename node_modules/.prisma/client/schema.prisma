generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Employee {
  id        Int      @id @default(autoincrement())
  fullName  String
  jobTitle  String
  country   String
  salary    Float
  createdAt DateTime @default(now())
}
